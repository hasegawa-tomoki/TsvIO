{
    "name": "hasegawa-tomoki/tsvio",
    "description": "Shell command to import / export database table records from / to tsv filles. CakePHP3 plugin.",
    "type": "cakephp-plugin",
	"keywords": ["cakephp", "import", "export"], 
	"license": "MIT", 
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Tsvio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tsvio\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
